            send ""
            play sound "minecraft:block.note_block.bass" at volume 100 to player

command /userevive:
    trigger:
        if {revivaltokens::%player's uuid%} >= 1:
            if {tokenusage} is true:
                remove 1 from {revivaltokens::%player's uuid%}
                broadcast ""
                broadcast "&b&lREVIVE: &f%sender%&7 has used an revival token!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players
            else:
                send ""
                send "&c&lWOAH!&7 You cannot use revival tokens anymore!"
                send ""
                play sound "minecraft:block.note_block.bass" at volume 100 to player
        else:
            send ""
            send "&c&lWOAH!&7 You don't have enough revival tokens!"
            send ""
            play sound "minecraft:block.note_block.bass" at volume 100 to player

command /tokenusage [<text>]:
    permission: lazycore.admin
    permission message: &c&lNO PERMISSION!&7 You cannot use this command because you aren't a staff member.
    trigger:
        if arg-1 is "enable", "true" or "yes":
            if {tokenusage} isn't set:
                set {tokenusage} to true
                broadcast ""
                broadcast "&b&lTOKEN: &f%sender%&7 has &aenabled&7 tokens!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players
            else if {tokenusage} is set:
                send ""
                send "&c&lWOAH!&7 Tokens are already enabled!"
                send ""
                play sound "minecraft:block.note_block.bass" at volume 100 to player
        if arg-1 is "disable", "false" or "no":
            if {tokenusage} is set:
                delete {tokenusage}
                set {tokenusage} to true
                broadcast ""
                broadcast "&b&lTOKEN: &f%sender%&7 has &aenabled&7 tokens!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players
            else if {tokenusage} isn't set:
                send ""
                send "&c&lWOAH!&7 Tokens are already disabled!"
                send ""
                play sound "minecraft:block.note_block.bass" at volume 100 to player
        if arg-1 isn't set:
            if {tokenusage} isn't set:
                set {tokenusage} to true
                broadcast ""
                broadcast "&b&lTOKEN: &f%sender%&7 has &aenabled&7 tokens!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players
            else if {tokenusage} is set:
                delete {tokenusage}
                broadcast ""
                broadcast "&b&lTOKEN: &f%sender%&7 has &cdisabled&7 tokens!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players


command /staffchat [<text>]:
    aliases: /sc, /staff
    permission: lazycore.admin
    permission message: &c&lNO PERMISSION!&7 You cannot use this command because you aren't a staff member.
    trigger:
        loop all players where [input has permission "lazycore.admin"]:
            if arg-1 is set:
                send "&b&lSTAFF: &f%player's display name% &8»&f %arg 1%" to loop-player
            else if arg-1 isn't set:
                if {staffchat.%player%} isn't set:
                    set {staffchat.%player%} to true
                    send ""
                    send "&b&lSTAFF: &7Your messages will now be sent to the staff chat!"
                    send ""
                    play sound "minecraft:block.note_block.bell" at volume 100 to player
                else if {staffchat.%player%} is set:
                    delete {staffchat.%player%}
                    send ""
                    send "&b&lSTAFF: &7Your messages will no longer be sent to the staff chat!"
                    send ""
                    play sound "minecraft:block.note_block.bell" at volume 100 to player

on chat:
    if {staffchat.%player%} is true:
        cancel event
        loop all players where [input has permission "lazycore.admin"]:
            send "&b&lSTAFF: &f%player's display name% &8»&f %message%" to loop-player


    permission message: &c&lNO PERMISSION!&7 You cannot use this command because you aren't a staff member.
    trigger:
        if arg-1 is set:
            if arg-2 is set:
                execute console command "/lp user %arg-1% parent set %arg-2%"
                send ""
                send "&c&lYAY!&7 You have successfully given &f%arg-1%&7 the rank &f%arg-2%&7."
                send ""
        if arg-1 or arg-2 isn't set:
            send ""
            send "&c&lWOAH!&7 You used the command wrong!"
            send ""       

command removerank [<player>] [<text>]:
    permission: op
    permission message: &c&lNO PERMISSION!&7 You cannot use this command because you aren't a staff member.
    trigger:
        if arg-2 isn't set:
            if arg-1 is set:
                execute console command "/lp user %arg-1% parent set default"
                send ""
                send "&c&lYAY!&7 You have successfully removed all ranks from &f%arg-1%&7."
                send ""
        if arg-1 isn't set:
            send ""
            send "&c&lWOAH!&7 You used the command wrong!"
            send ""
        if arg-2 is set:
            if arg-1 is set:
                execute console command "/lp user %arg-1% parent remove %arg-2%"
                send ""
                send "&c&lYAY!&7 You have successfully removed the &f%arg-2%&7 rank from &f%arg-1%&7!"
                send ""

# ---------------------------------------------------------------------------------------------------------------------------

on hunger meter change:
    if {hunger} isn't set:
        if "%region at player%" does not contain "ffanew":
            cancel event

on damage:
    if damage cause is fall:
        if {falldamage} isn't set:
            cancel event 

on place:
    if {build} isn't set:
        if "%region at player%" doesn't contain "{@ArenaRegion}":
            if player doesn't have permission "lazycore.admin":
                cancel event

on break:
    if player is in region "{@ArenaRegion}":
        if event-block is in region "{@ArenaRegion}":
            stop
        else:
            cancel event

on break:
    if {break} isn't set:
        if player doesn't have permission "lazycore.admin":
            cancel event

on jump:
    if {jump} is set:
        if player doesn't have permission "lazycore.admin":
            cancel event

on join:
    delete {visibility::%player%}
        
on flow:
    if {flowing} isn't set:
        cancel event

on chat:
    if {mutechat} is "staff":
        if sender doesn't have permission "lazycore.admin":
            cancel event
    else if {mutechat} is true:
        if sender doesn't have permission "lazycore.mutechatbypass" or "lazycore.admin":
            cancel event

on damage:
    if attacker doesn't have permission "lazycore.admin":
        if attacker is a player:
            if {pvp} isn't set:
                if victim is a player:
                    cancel event

on projectile hit:
    if projectile is an arrow:
        if "%region at shooter%" doesn't contain "ffanew":
            if {teleporting_arrow} is true:
                set {_arrow} to projectile
                wait 1 tick
                teleport shooter to {_arrow}
                clear projectile


command /mutechat [<text>]:
    permission: lazycore.admin
    permission message: &c&lNO PERMISSION!&7 You cannot use this command because you aren't a staff member.
    trigger:
        if arg-1 is "enable", "on" or "true":
            if {mutechat} isn't set:
                set {mutechat} to true
                broadcast ""
                broadcast "&b&lMUTE: &f%player%&7 has &cmuted&7 the chat!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players
            else if {mutechat} is set:
                send ""
                send "&c&lWOAH!&7 Chat has already been muted!"
                send ""
                play sound "minecraft:block.note_block.bass" at volume 100 to player
        if arg-1 is "disable", "off" or "false":
            if {mutechat} is set:
                delete {mutechat}
                broadcast ""
                broadcast "&b&lMUTE: &f%player%&7 has &aunmuted&7 the chat!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players
            else:
                send "%nl%&c&lWOAH!&7 Chat is already unmuted!%nl%"
                play sound "minecraft:block.note_block.bass" at volume 100 to player
        if arg-1 is "staff":
            if {mutechat} isn't "staff":
                set {mutechat} to "staff"
                broadcast "%nl%&b&lMUTE: &f%player%&7 has &aenabled&7 staff only chat!%nl%"
                play sound "minecraft:block.note_block.bell" at volume 100 to players
            else:
                delete {mutechat}
                broadcast "%nl%&b&lMUTE: &f%player%&7 has &aunmuted&7 the chat!%nl%"
                play sound "minecraft:block.note_block.bell" at volume 100 to players
        if arg-1 isn't set:
            if {mutechat} is set:
                delete {mutechat}
                broadcast ""
                broadcast "&b&lMUTE: &f%player%&7 has &aunmuted&7 the chat!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players
            else if {mutechat} isn't set:
                set {mutechat} to true
                broadcast ""
                broadcast "&b&lMUTE: &f%player%&7 has &cmuted&7 the chat!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players


command /number [<integer>] [<integer>] [<text>]:
    permission: lazycore.admin
    permission message: &c&lNO PERMISSION!&7 You cannot use this command because you aren't a staff member.
    trigger:
        if arg-3 isn't set:
            if arg-1 is set:
                if arg-2 is set:
                    clear {_num}
                    set {_num} to random integer between arg-1 and arg-2
                    broadcast ""
                    broadcast "&b&lNUMBER: &f%sender%&7 has generated an number from between &f%arg-1%&7 and &f%arg-2%&7! The number is &f%{_num}%&7!"
                    broadcast ""
                    play sound "minecraft:block.note_block.bell" at volume 100 to players
                else:
                    send ""
                    send "&c&lWOAH!&7 You used the command wrong!"
                    send ""
                    play sound "minecraft:block.note_block.bass" at volume 100 to player
            else:   
                send ""
                send "&c&lWOAH!&7 You used the command wrong!"
                send ""
                play sound "minecraft:block.note_block.bass" at volume 100 to player
        if arg-3 is "private":
            if arg-1 is set:
                if arg-2 is set:
                    clear {_num}
                    set {_num} to random integer between arg-1 and arg-2
                    send ""
                    send "&b&lNUMBER: &7You have generated an number from between &f%arg-1%&7 and &f%arg-2%&7! The number is &f%{_num}%&7!"
                    send ""
                    play sound "minecraft:block.note_block.bell" at volume 100 to sender

command /spawn:
    trigger:
        if {alive::*} doesn't contain sender:
            teleport sender to {spawn}
        else if {alive::*} contains sender:
            send ""
            send "&c&lWOAH!&7 You cannot go to spawn, because you are alive."
            send ""
            play sound "minecraft:block.note_block.bass" at volume 100 to sender


command /revive [<player>]:
    permission: lazycore.admin
    permission message: &c&lNO PERMISSION!&7 You cannot use this command because you aren't a staff member.
    trigger:
        if arg-1 isn't set:
            send ""
            send "&c&lWOAH!&7 You are using the command wrong! You need to name the player that you want to revive."
            send ""
            play sound "minecraft:block.note_block.bass" at volume 100 to players
        if arg-1 is set:
            if {alive::*} doesn't contain arg-1:
                wait 1 tick
                add arg-1 to {alive::*}
                wait 1 tick
                remove arg-1 from {dead::*}
                set {status::%arg-1's uuid%} to "&aᴀʟɪᴠᴇ"
                teleport arg-1 to sender
                broadcast ""
                broadcast "&b&lREVIVE: &f%player%&7 has revived &f%arg-1%!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players
            else:
                send ""
                send "&c&lWOAH!&7 %arg-1%&7 is already alive!"
                send ""
                play sound "minecraft:block.note_block.bass" at volume 100 to players

command /reviveall [<text>]:
    permission: lazycore.admin
    permission message: &c&lNO PERMISSION!&7 You cannot use this command because you aren't a staff member.
    trigger:
        if arg-1 isn't set:
            broadcast ""
            broadcast "&b&lREVIVE: &f%player%&7 has revived everyone!"
            broadcast ""
            play sound "minecraft:block.note_block.bell" at volume 100 to players
            loop all players:
                if loop-player is not player:
                    if {dead::*} contains loop-player:
                        add loop-player to {alive::*}
                        set {status::%loop-player's uuid%} to "&aᴀʟɪᴠᴇ"
                        wait 1 tick
                        remove loop-player from {dead::*}
                        teleport loop-player to sender
        if arg-1 is "fast":
            broadcast ""
            broadcast "&b&lREVIVE: &f%player%&7 has revived everyone!"
            broadcast ""
            play sound "minecraft:block.note_block.bell" at volume 100 to players
            loop all players:
                if loop-player is not player:
                    if {dead::*} contains loop-player:
                        add loop-player to {alive::*}
                        set {status::%loop-player's uuid%} to "&aᴀʟɪᴠᴇ"
                        wait 1 tick
                        remove loop-player from {dead::*}
                        teleport loop-player to sender
        if arg-1 is "slow":
            broadcast ""
            broadcast "&b&lREVIVE: &f%player%&7 has revived everyone!"
            broadcast ""
            play sound "minecraft:block.note_block.bell" at volume 100 to players
            loop all players:
                if loop-player is not player:
                    if {dead::*} contains loop-player:
                        add loop-player to {alive::*}
                        set {status::%loop-player's uuid%} to "&aᴀʟɪᴠᴇ"
                        loop {dead::*}:
                            wait 5 ticks
                            teleport loop-player-1 to sender
                            wait 1 tick
                            remove loop-player-1 from {dead::*}


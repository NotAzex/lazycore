command /giveskip [<player>] [<integer>]:
    permission: lazycore.admin
    permission message: &c&lNO PERMISSION!&7 You cannot use this command because you aren't a staff member.
    trigger:
        if arg-1 is set:
            if arg-2 is set:
                add arg-2 to {skips::%arg-1's uuid%}
                broadcast ""
                broadcast "&b&lSKIP: &f%sender%&7 gave &f%arg-1%&7 &7%arg-2%&7 skip(s)!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players
            else:
                add 1 to {skips::%arg-1's uuid%}
                broadcast ""
                broadcast "&b&lSKIP: &f%sender%&7 gave &f%arg-1%&7 &71 skip!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players          
        else:
            add 1 to {skips::%player's uuid%}
            send "&b&lSKIP:&7 You have given yourself &f1&7 skip!"
            play sound "minecraft:block.note_block.bell" at volume 100 to player

command /removeskip [<player>] [<integer>]:
    permission: lazycore.admin
    permission message: &c&lNO PERMISSION!&7 You cannot use this command because you aren't a staff member.
    trigger:
        if arg-1 is set:
            if arg-2 is set:
                remove arg-2 from {skips::%arg-1's uuid%}
                broadcast ""
                broadcast "&b&lSKIP: &f%sender%&7 has removed &f%arg-2%&7 skips from &f%arg-1%&7!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players
            else:
                remove 1 from {skips::%arg-1's uuid%}
                broadcast ""
                broadcast "&b&lSKIP: &f%sender%&7 has removed &f1&7 skip from &f%arg-1%&7!"
                broadcast ""
                play sound "minecraft:block.note_block.bell" at volume 100 to players
        else:
            send "&c&lWOAH!&7 You need to name the player that you want to remove skips from!"
            play sound "minecraft:block.note_block.bass" at volume 100 to player

command /skips [<player>]:
    trigger: 
        send "&b&lSKIP: &f%arg-1%&7 has &f%{skips::%arg-1's uuid%}%&7 skips!"
        play sound "minecraft:block.note_block.bass" at volume 100 to player

on tab complete of "/alive" and "/dead":
    set tab completions for position 1 to "detailed"
        
on tab complete of "/hunger", "/teleportingarrow", "/pvp", "/mutechat", "/build", "/flow", "/break", "/jump", "/tokenusage" and "/falldamage":
    set tab completions for position 1 to "on" and "off"
        
on tab complete of "/hide":
    if player has permission "lazycore.admin":
        set tab completions for position 1 to "host"
    
on tab complete of "/tpall", "/tpdead", "/reviveall" and "/tpalive":
    if player has permission "lazycore.admin":
        set tab completions for position 1 to "slow" and "fast"
command /skip [<text>]:
    trigger:
        if arg-1 isn't set:
            if {skips::%player's uuid%} >= 1:
                if {skipusage} is true:
                    remove 1 from {skips::%player's uuid%}
                    add player to {skipsrecent::*}
                    loop all players:
                        if loop-player has permission "op":
                            send ""
                            send "&b&lSKIP: &f%player%&7 has used an skip! Will you &aaccept&7 it or &cdeny&7 it?"
                            send ""
                else:
                    send ""
                    send "&c&lWOAH!&7 Skips are disabled at the moment!"
                    send ""
                    play sound "minecraft:block.note_block.bass" at volume 100 to player
        if arg-1 is "accept":
            if player has permission "lazycore.admin":
                if size of {skipsrecent::*} isn't 0:
                    teleport players in {skipsrecent::*} to sender
                    add sender to {skipsrecentowner::*}
                    send "&b&lSKIP: &f%{skipsrecentowner::*}%&7 has &aaccepted&7 your skip!"
                    play sound "minecraft:block.note_block.bell" at volume 100 to players in {skipsrecent::*}
                    clear {skipsrecent::*}
                    clear {skipsrecentowner::*}
                else:
                    send "&c&lWOAH!&7 No one has used an skip!"
                    play sound "minecraft:block.note_block.bass" at volume 100 to player
        if arg-1 is "deny":
            if player has permission "lazycore.admin":
                if size of {skipsrecent::*} isn't 0:
                    add sender to {skipsrecentowner::*}
                    loop players in {skipsrecent::*}:
                        add 1 to {skips::%loop-player's uuid%}
                        send "&b&lSKIP: &f%{skipsrecentowner::*}%&7 didn't accept your skip!"
                        play sound "minecraft:block.note_block.bass" at volume 100 to loop-player
                        clear {skipsrecent::*}
                        clear {skipsrecentowner::*}
                else:
                    send "&c&lWOAH!&7 No one has used an skip!"
                    play sound "minecraft:block.note_block.bass" at volume 100 to player


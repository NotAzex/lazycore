options: # Remember to use "true" or "false"!
    Title: &7Default  # replace the IP with your server's IP (or something else!)
    IP: &7Default # replace the text with your own text
    HidePlugins: true # Will only show that you are using the LazyCore skript (with an discord invite) when you type /plugins if this is set to true.
    CustomJoinMessage: false # Will send an random join message whenever an player joins if this is enabled.
    CustomLeaveMessage: false # Will do the same thing as "CustomJoinMessage", but it sends an leave message instead.
    ArenaRegion: arena # Put in your arena's region name.
    ApplyNightVision: true # Will automatically apply night vision without particles to player when they die or join the server.

on load:
    send "" to console
    send "                            &x&0&8&4&c&f&b&lʟ&x&2&0&6&4&f&b&lᴀ&x&3&7&7&c&f&c&lᴢ&x&4&f&9&4&f&c&lʏ&x&6&6&a&b&f&c&lᴄ&x&7&e&c&3&f&c&lᴏ&x&9&5&d&b&f&d&lʀ&x&a&d&f&3&f&d&lᴇ" to console
    send "" to console
    send "                 &7Succesfully loaded the &x&0&8&4&c&f&b&lʟ&x&2&0&6&4&f&b&lᴀ&x&3&7&7&c&f&c&lᴢ&x&4&f&9&4&f&c&lʏ&x&6&6&a&b&f&c&lᴄ&x&7&e&c&3&f&c&lᴏ&x&9&5&d&b&f&d&lʀ&x&a&d&f&3&f&d&lᴇ&7!" to console
    send "" to console
    play sound "minecraft:block.note_block.bit" at volume 100 to player

on join:
    if {skips::%player's uuid%} isn't set:
        set {skips::%player's uuid%} to 0
    if {revivaltokens::%player's uuid%} isn't set:
        set {revivaltokens::%player's uuid%} to 0
    if {alive::*} contains player:
        remove player from {alive::*}
    if {dead::*} doesn't contain player:
        add player to {dead::*}
        set {status::%player's uuid%} to "&cᴅᴇᴀᴅ"
    teleport player to {spawn}
    if {@ApplyNightVision} is true:
        apply night vision without particles to player for 1 year
    while player is online:
        set title of player's scoreboard to "{@Title}"
        set line 14 of player's scoreboard to ""
        set line 13 of player's scoreboard to "&b&lᴘʟᴀʏᴇʀ"
        set line 12 of player's scoreboard to " &8•&7 ᴛᴏᴋᴇɴs: &b%{revivaltokens::%player's uuid%}%"
        set line 11 of player's scoreboard to " &8•&7 sᴛᴀᴛᴜs: &b%{status::%player's uuid%}%"
        set line 10 of player's scoreboard to ""
        set line 9 of player's scoreboard to "&b&lᴇᴠᴇɴᴛ"
        set line 8 of player's scoreboard to " &8•&7 ᴇᴠᴇɴᴛ: &b%{event::*}%"
        set line 7 of player's scoreboard to " &8•&7 ᴀʟɪᴠᴇ: &b%size of {alive::*}%"
        set line 6 of player's scoreboard to ""
        set line 5 of player's scoreboard to "&b&lsᴇʀᴠᴇʀ"
        set line 4 of player's scoreboard to " &8•&7 ᴏɴʟɪɴᴇ: &b%size of all players%"
        set line 3 of player's scoreboard to " &8•&7 ᴅᴇᴀᴅ: &b%size of {dead::*}%"
        set line 2 of player's scoreboard to ""
        set line 1 of player's scoreboard to "{@IP}"
        wait 1 tick

on join:
    if {@CustomJoinMessage} is true:
        set {joinlist::*} to "&a&l+&7 • &7Oh, &f%player%&7 has joined!", "&a&l+&7 • &7Everyone, say hello to &f%player%&7!", "&a&l+&7 • &7Hey there, &f%player%&7! Welcome!", "&a&l+&7 • &7Say hello to &f%player%&7!" and "&a&l+&7 • &7Glad you joined, &f%player%&7!"
        set join message to random element of {joinlist::*}
        play sound "minecraft:block.note_block.pling" at pitch 2 to players

on leave:
    if {@CustomLeaveMessage} is true:
        set {leavelist::*} to "&c&l-&7 • &7&f%player%&7 has left, that's a shame.", "&c&l-&7 • &f%player%&7 has left us...", "&c&l-&7 • &f%player%&7 left." and "&c&l-&7 • &7Oh, &f%player%&7 left."
        set leave message to random element of {leavelist::*}
        play sound "minecraft:block.note_block.bass" at volume 100 to players

on death of player:
    if {alive::*} contains victim:
        add victim to {dead::*}
        remove victim from {alive::*}
        set {status::%victim's uuid%} to "&cᴅᴇᴀᴅ"
    if {dead::*} doesn't contain victim:
        add victim to {dead::*}
        set {status::%victim's uuid%} to "&cᴅᴇᴀᴅ"

on leave:
    if {alive::*} contains player:
        remove player from {alive::*}
    remove player from {dead::*}

on respawn:
    if {@ApplyNightVision} is true:
        set respawn location to {spawn}
        wait 1 tick
        apply night vision without particles to player for 1 year
    set respawn location to {spawn}

on death of player:
    if {reviverecent::*} doesn't contain victim:
        add victim to {reviverecent::*}
        wait 30 seconds
        remove victim from {reviverecent::*}

command /timer [<integer>] [<text>]:
    permission: lazycore.admin
    permission message: &c&lNO PERMISSION!&7 You cannot use this command because you aren't a staff member.
    trigger:
        if arg-1 is an integer:
            if {_timer} isn't set:
                if arg-2 is "seconds" or "second":
                    set {_timer} to arg-1
                    while {_timer} > 0:
                        broadcast "&b&lTIMER:&7 &f%{_timer}%&7 seconds left!"
                        play sound "minecraft:block.note_block.bit" at volume 100 to players
                        wait 1 second
                        remove 1 from {_timer}
                    broadcast ""
                    broadcast "&b&lTIMER:&7 Timer is at &f0&7!"
                    broadcast ""
                    play sound "minecraft:block.note_block.bell" at volume 100 to players
                    delete {_timer}

                else if arg-2 is "minutes" or "minute":
                    set {_timer} to arg-1
                    while {_timer} > 0:
                        broadcast "&b&lTIMER:&7 &f%{_timer}%&7 minutes left!"
                        play sound "minecraft:block.note_block.bit" at volume 100 to players
                        wait 1 minute
                        remove 1 from {_timer}
                    broadcast ""
                    broadcast "&b&lTIMER:&7 Timer is at &f0&7!"
                    broadcast ""
                    play sound "minecraft:block.note_block.bell" at volume 100 to players
                    delete {_timer}
                else if arg-2 isn't "minutes", "minute", "seconds" or "second":
                    send ""
                    send "&c&lWOAH!&7 You need to specify if you want the timer to be in seconds or minutes! Like for example: &f/timer 60 seconds&7."
                    send ""
                    play sound "minecraft:block.note_block.bass" at volume 100 to player
            else:
                send ""
                send "&c&lWOAH!&7 You already have an on-going timer!"
                send ""
                play sound "minecraft:block.note_block.bass" at volume 100 to player
        else:
            send ""
            send "&c&lWOAH!&7 You must put in the amount of seconds! Like for example: &f/timer 60 minutes / seconds&7."
            send ""
            play sound "minecraft:block.note_block.bass" at volume 100 to player

